<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="0">

        <!-- Sequences for sale.order -->
        <record id="seq_type_case_sheet" model="ir.sequence.type">
            <field name="name">Case Sheet</field>
            <field name="code">case.sheet</field>
        </record>

        <record id="seq_case_sheet" model="ir.sequence">
            <field name="name">Case Sheet</field>
            <field name="code">case.sheet</field>
            <field name="prefix"></field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <record id="seq_type_case_tasks_line_slno" model="ir.sequence.type">
            <field name="name">Case Tasks Slno</field>
            <field name="code">case.tasks.line</field>
        </record>

        <record id="seq_case_tasks_line_slno" model="ir.sequence">
            <field name="name">Case Tasks Slno</field>
            <field name="code">case.tasks.line</field>
            <field name="prefix"></field>
            <field name="padding">0</field>
            <field name="slno" eval="True"/>
        </record>
        
        
        <record id="seq_type_inward_register_slno" model="ir.sequence.type">
            <field name="name">Inward Register Slno</field>
            <field name="code">inward.register</field>
        </record>

        <record id="seq_inward_register_slno" model="ir.sequence">
            <field name="name">Inward Register Slno</field>
            <field name="code">inward.register</field>
            <field name="prefix">INW</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        
        
        <record id="seq_type_outward_register_slno" model="ir.sequence.type">
            <field name="name">Outward Register Slno</field>
            <field name="code">outward.register</field>
        </record>

        <record id="seq_outward_register_slno" model="ir.sequence">
            <field name="name">Outward Register Slno</field>
            <field name="code">outward.register</field>
            <field name="prefix">OUTW</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        
        
        <record id="seq_type_bulk_casesheet_slno" model="ir.sequence.type">
            <field name="name">Bulk Casesheet Slno</field>
            <field name="code">bulk.casesheet</field>
        </record>

        <record id="seq_bulk_casesheet_slno" model="ir.sequence">
            <field name="name">Bulk Casesheet Slno</field>
            <field name="code">bulk.casesheet</field>
            <field name="prefix">LOT</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</openerp>
